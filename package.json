{
  "name": "nuxt-app",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev --port 8080",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "test:unit": "vitest",
    "test:jsdom": "VITEST_DOM_ENV=jsdom npm test:unit --run",
    "test:happy-dom": "VITEST_DOM_ENV=happy-dom npm test:unit --run"
  },
  "devDependencies": {
    "@types/node": "^18",
    "nuxt": "^3.5.1",
    "nuxt-vitest": "^0.7.1"
  },
  "overrides": {
    "vue": "latest"
  },
  "dependencies": {
    "@mdi/font": "^7.2.96",
    "@pinia/nuxt": "^0.4.11",
    "happy-dom": "^10.3.2",
    "pinia": "^2.1.3",
    "sass": "^1.62.1",
    "vitest": "^0.30.1",
    "vuetify": "^3.3.1"
  }
}
